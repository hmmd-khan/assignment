import java.util.*;
class area{
    public static void main(String[]args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("enter the number");
        int x;
        int sum=0;
        for (int i = 1; i <= 5; i++) {
            x = sc.nextInt();
            sum+=x;
        }
        System.out.println(sum);
    }
}
